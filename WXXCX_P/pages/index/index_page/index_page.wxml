<view class="top">
    <view class="pages-row">
      <navigator hover-class="none" url="/pages/news/news_index/news_index" class="pages-item">
        <view>
          <image class="pages-icon" src="{{newspaper}}"></image>
          <text>民大新闻</text>
        </view>
      </navigator>
      <navigator hover-class="none" url="/pages/developing/developing" class="pages-item">

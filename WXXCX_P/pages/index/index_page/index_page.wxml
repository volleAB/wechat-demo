          <image class="pages-icon" src="{{weixin}}"></image>
          <text>官方微信</text>
        </view>
      </navigator>
      <navigator hover-class="none" url="/pages/weibo/weibo" class="pages-item">
        <view>
          <image class="pages-icon" src="{{weibo}}"></image>
          <text>官方微博</text>
        </view>
      </navigator>
      <navigator url="/pages/broadcast/broadcast_index/broadcast_index" class="pages-item">
        <view>
          <image class="pages-icon" src="{{broadcast}}"></image>
          <text>民大广播</text>
        </view>
      </navigator>
    </view>

    <view class="pages-row">
      <navigator url="/pages/TV/TV_index/TV_index" class="pages-item">
        <view>
          <image class="pages-icon" src="{{television}}"></image>
          <text>民大电视</text>
        </view>
      </navigator>
      <navigator url="/pages/developing/developing" class="pages-item">
        <view>
          <image class="pages-icon" src="{{live}}"></image>
          <text>双塔直播</text>
        </view>
      </navigator>
      <navigator url="/pages/weizong/weizong_index/weizong_index" class="pages-item">
        <view>
          <image class="pages-icon" src="{{magazine}}"></image>
          <text>微众杂志</text>
        </view>
      </navigator>
      <navigator url="/pages/developing/developing" class="pages-item">
        <view>
          <image class="pages-icon" src="{{other}}"></image>
          <text>其 他</text>
        </view>
      </navigator>
    </view>
  </view>
